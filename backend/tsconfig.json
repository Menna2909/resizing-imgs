{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*", "src/tests", "dist/tests/postReqSpec.js", "dist/tests/getReqSpec.js"],
  "exclude": ["node_modules", "./dist", "spec"]
}
